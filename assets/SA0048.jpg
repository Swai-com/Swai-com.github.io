    function loadStorePupop(iditem, idcolor, itemsize, provinceid, itemname = "") {
        //if (itemsize == null || itemsize == undefined) {
        //    itemsize = $('.kichco.selected').attr('data-size');
        //    if (itemsize == undefined) {
        //        itemsize = "0S";
        //    }
        //}
        var params = {
            type: 'POST',
            url: '/CategoryV2/CheckItemInSrore',
            data: JSON.stringify({
                itemid: iditem,
                colorid: idcolor,
                sizeitem: itemsize,
                province: provinceid
            }),
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            async: true,
            success: function (result) {
                if (result.success) {
                    $('#selectsize').empty();
                    $.each(result.dtbSize, function (name, value) {
                        $('#selectsize').append($('<option>', {
                            value: value.SIZE,
                            text: value.SIZE
                        }));
                    });
                    $('#selectsize').val(itemsize);
                    $('#my_image').attr('src', result.dtbImageLink.shift().LGIMAGE);
                    $('#colorchange').val(result.dtbImageLink.shift().COLORID);
                    $('.street').remove();
                    $('.timeStore').remove();
                    result.Count > 0 ? $('#stock-sp').text(result.Count) : $('#stock-sp').text('0');
                    var number = 0;
                    $.each(result.store, function (name, values) {
                        $("<span class='street'> <img class='img-icon' src = '/Images/chitiet/location.png'style ='width: 20px;'>" + values.STOREADDRESS + "<div id='bookingSP" + number + "' data-id = " + iditem + " " + "data-colorid = " + idcolor + " " + "data-size=" + result.dtbSize[0].SIZE + " " + "data-store=" + values.STOREID + " style='display: contents;'" + "> <a style='color:#007bff' href='#'>&nbsp; Có hàng, đặt giữ tại đây</a></div></span>").appendTo(".stock");
                        $("<p class='timeStore'><img class='img-icon' src='/Images/chitiet/open.png' style='width: 20px;'> 8 giờ - 22 giờ(kể cả CN và ngày lễ) </p>").appendTo(".stock");
                        $('#bookingSP' + number).click(function () {
                            var storeid = $(this).attr("data-store");
                            var cart = {};
                            cart.ITEMID = iditem;
                            cart.COLORID = idcolor;
                            cart.SIZE = itemsize;
                            cart.STOREID = storeid;
                            //console.log(cart);
                            cart.ISTOCHECK = "Category";
                            cart.ITEMNAME = itemname;

                            var params = {
                                type: 'POST',
                                url: '/gio-hang/them.jsp',
                                data: {
                                    cart: cart,
                                    storeid: storeid,
                                    provinceid: provinceid
                                },
                                dataType: 'json',
                                success: function (result) {
                                    if (result.success) {
                                        //localStorage.setItem("key", "true");
                                        //setCartPopup(900000);
                                        document.location.href = "/gio-hang";
                                    } else {
                                        alert(result.responseText);
                                    }
                                },
                                error: function (XMLHttpRequest, textStatus) {
                                }
                            };
                            $.ajax(params);
                        })
                        number++;
                    });
                    if (number == 0) {
                        $("<span class='street'> <img class='img-icon' src = '/Images/chitiet/location.png'style ='width: 20px;'><div id='bookingSP'>Đặt hàng online giao hàng tận nơi <a href='#'>&nbsp;tại đây!</a></div></span>").appendTo(".stock");
                        $('#bookingSP').click(function () {
                            var storeid = $(this).attr("data-store");
                            var cart = {};
                            cart.ITEMID = iditem;
                            cart.COLORID = idcolor;
                            cart.SIZE = itemsize;
                            cart.STOREID = storeid;
                            cart.ISTOCHECK = "Category";

                            var params = {
                                type: 'POST',
                                url: '/gio-hang/them.jsp',
                                data: {
                                    cart: cart,
                                    storeid: storeid,
                                    provinceid: provinceid
                                },
                                dataType: 'json',
                                success: function (result) {
                                    if (result.success) {
                                        //localStorage.setItem("key", "true");
                                        //setCartPopup(900000);
                                        document.location.href = "/gio-hang";
                                     
                                    } else {
                                        alert(result.responseText);
                                    }
                                },
                                error: function (XMLHttpRequest, textStatus) {
                                }
                            };
                            $.ajax(params);
                        })
                    }

                };
            },
            error: function (XMLHttpRequest, textStatus) {
            }
        };
        $.ajax(params);
       
    }

