    function LoadPopupAddcart(strid, strcolorid, addcart) {
        var params = {
            type: 'POST',
            url: '/CategoryV2/SelectColor',
            data: { nameid: strid, colorid: strcolorid },
            dataType: 'json',
            success: function (result) {
                if (result) {
                    //console.log(result);
					var variant = "";
                    $('#addcart_image').attr('src', result.itemMake.LISTIMAGES[0]);
                    var htmlSize = '';
                    if (result.Sizes && result.Sizes.length > 0) {
                        variant = result.Sizes[0].PRODUCTID.substring(0, 14);
						if (result.itemMake.MAINGROUPID === 5050 || result.itemMake.MAINGROUPID === 6050 || result.itemMake.MAINGROUPID === 8050) {
                            var shoessize = 35;
                            for (var b = 0; b <= result.Sizes.length - 1; b++) {
                                var shoes = result.Sizes.filter(function (q) { return q.SIZE === shoessize.toString(); });
                                if (shoes !== null && shoes.length > 0) {
                                    if (shoes[0].AVARIABLEQUANTITY >= 1 && !shoes[0].ISLOCKEDORDER && !result.islockorder) {
                                        htmlSize += '<div class="size-item-detailv2';
                                    } else {
                                        htmlSize += '<div class="size-item-detailv2-disable';
                                    }
                                } else {
                                    htmlSize += '<div class="size-item-detailv2-disable';
                                }
                                htmlSize += '">' + shoessize + '</div>';
                                shoessize++;
                            }
                            $('.addcart-size').html(htmlSize);
                        } else {
                            htmlSize += '<div class="size-item-detail seleted-active-sizev2">' + result.Sizes[0].SIZE + '</div>';
                            $('.item-list-size-mobile').html(htmlSize);
                        }
                    }
                    if (addcart === 0) {
                        $('.addcart-footer').html('<button type="button" class="btn btn-addcart btn-add-cart-view" data-itemid="' + result.itemMake.ITEMID + '" id="btnaddcartv2" data-nameid="' + strid + '" data-colorid="' + strcolorid + '" data-itemname= "' + result.itemMake.ITEMNAME + '" data-saleprice= "' + result.itemMake.SALEPRICE + '" data-retailprice= "' + result.itemMake.RETAILPRICE + '" data-maingroupname= "' + result.itemMake.MAINGROUPNAME + '" data-variant= "' + variant +'" >THÊM VÀO GIỎ HÀNG</button>');
                    } else {
                        $('.addcart-footer').html('<button type="button" class="btn btn-addcart btn-add-cart-view" data-itemid="' + result.itemMake.ITEMID + '" id="btnbuycartv2" data-nameid="' + strid + '" data-colorid="' + strcolorid + '" data-itemname= "' + result.itemMake.ITEMNAME + '" data-saleprice= "' + result.itemMake.SALEPRICE + '" data-retailprice= "' + result.itemMake.RETAILPRICE + '" data-maingroupname= "' + result.itemMake.MAINGROUPNAME + '" data-variant= "' + variant +'" >MUA NGAY</button>');
                    }

                } else {
                    console.log('false rồi');
                }
            },
            error: function (XMLHttpRequest, textStatus) {
            }
        };
        $.ajax(params);
    }

